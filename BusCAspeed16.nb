        RowBox[{
         RowBox[{"Print", "[", "\"\<\\tVery far.\>\"", "]"}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"Which", "[", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"v", ">=", 
            RowBox[{"2", 
             RowBox[{"Abs", "[", "am", "]"}]}]}], 
           RowBox[{"(*", "fast", "*)"}], ",", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"Print", "[", "\"\<\\t\\tFast, brake.\>\"", "]"}], ";", 
            "\[IndentingNewLine]", 
            RowBox[{"movebottom", "[", 
             RowBox[{"bottom", ",", 
              RowBox[{"brake", "[", "v", "]"}]}], "]"}]}], ",", 
           "\[IndentingNewLine]", 
           RowBox[{"v", "==", 
            RowBox[{"Abs", "[", "am", "]"}]}], 
           RowBox[{"(*", 
            RowBox[{
             RowBox[{"Not", " ", "so", " ", "fast"}], ",", " ", 
             RowBox[{"maybe", " ", "hold"}]}], "*)"}], ",", 
           "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"Print", "[", "\"\<\\t\\tNot so fast, hold?\>\"", "]"}], 
            ";", "\[IndentingNewLine]", 
            RowBox[{"movebottom", "[", 
